{
  "extends": "astro/tsconfigs/strict",
  "exclude": ["public", "dist"],
  "compilerOptions": {
    "strictNullChecks": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@/solid/*": ["src/components/solidjs/*"],
    },
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
  }
}
